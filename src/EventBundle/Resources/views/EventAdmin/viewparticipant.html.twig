{% endblock %}

{% block body %}
    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-header row">
            </div>

            <!-- PUT BODY HERE -->
            <div class="card-header">
                <div class="col-12"><h4 class="card-title">Participation  </h4>
                </div>
            </div>
            <div class="content-body">
                <div class="row match-height">
                    <section id="decks">
                        <div class="row">
                            <div class="col-12">
                                {% if formations.titreEvent is defined %}
                                    <div class="">
                                        <div class="">
                                            <div class="card">
                                                <div class="card-body">
                                                    <div class="">
                                                    <h5 class="text-bold-600 mt-1"> <span class="blue-grey">{{ formations.titreEvent }}</span></h5>
                                                    <br>
                                                    <h6 class="text-bold-600 mt-1"> Type: <span class="blue-grey">{{ formations.TypeEvent }}</span></h6>
                                                    <h6 class="text-bold-600 mt-1"> Numero: <span class="blue-grey">{{ formations.numeroEvent }}</span></h6>
                                                    <h6 class="text-bold-600 mt-1"> Date: <span class="blue-grey">{{ formations.dateEvent|date('Y-m-d') }}</span></h6>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                {% else %}
                                    {% for i in formations %}
                                        <div class="col-12">
                                            <div class="">
                                                <div class="card">
                                                    <div class="card-body">
                                                        <div class="col-12">
                                                        <h5 class="text-bold-600 mt-1"> <span class="blue-grey">{{ i.titreEvent }}</span></h5>
                                                        <br>
                                                        <h6 class="text-bold-600 mt-1"> Type: <span class="blue-grey">{{ i.TypeEvent }}</span></h6>
                                                        <h6 class="text-bold-600 mt-1"> Numero: <span class="blue-grey">{{ i.numeroEvent }}</span></h6>
                                                        <h6 class="text-bold-600 mt-1"> Date: <span class="blue-grey">{{ i.dateEvent|date('Y-m-d') }}</span></h6>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    {% endfor %}
                                {% endif %}
                            </div>
                        </div>
                    </section>
                </div>




            </div>

        </div>
    </div>
    </div>


    </div>
    </div>





    <!--/ END BODY -->
    </div>
    </div>





{% endblock %}


